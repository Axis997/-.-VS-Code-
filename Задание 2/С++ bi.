#include <iostream>
#include <queue>
#include <vector>
using namespace std;

int main() {
    priority_queue<int, vector<int>, greater<int>> heap; // min-heap
    heap.push(5);
    heap.push(3);
    heap.push(7);
    heap.push(1);

    while (!heap.empty()) {
        cout << heap.top() << " ";
        heap.pop();
    }
    return 0;
}
