import java.util.*;

public class HashDemo {
    public static void main(String[] args) {
        Map<String,Integer> map = new HashMap<>();
        map.put("apple", 10);
        map.put("banana", 20);

        System.out.println(map.get("apple"));   // 10
        System.out.println(map.containsKey("banana")); // true

        map.remove("apple");
        System.out.println(map);
    }
}
